{
    "resource": {
        "aws_security_group": {
            "allow_ssh": {
                "description": "Allow SSH inbound from anywhere",
                "ingress": {
                    "cidr_blocks": [
                        "0.0.0.0/0"
                    ],
                    "from_port": 22,
                    "protocol": "tcp",
                    "to_port": 22
                },
                "name": "allow_ssh",
                "vpc_id": "arn"
            }
        }
    }
}